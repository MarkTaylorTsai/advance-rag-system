# RAG System environment variables

# General
ENV=dev
DATA_DIR=/path/to/data
DATABASE_URL=sqlite:////path/to/data/webui.db
DEFAULT_LOCALE=

# RAG embedding prefixes
RAG_EMBEDDING_QUERY_PREFIX=
RAG_EMBEDDING_CONTENT_PREFIX=
RAG_EMBEDDING_PREFIX_FIELD_NAME=
RAG_EMBEDDING_MODEL_AUTO_UPDATE=True
RAG_EMBEDDING_MODEL_TRUST_REMOTE_CODE=True
RAG_RERANKING_MODEL_AUTO_UPDATE=True
RAG_RERANKING_MODEL_TRUST_REMOTE_CODE=True

# Retrieval / Vector DB selection
VECTOR_DB=chroma

# Chroma
CHROMA_DATA_PATH=/path/to/data/vector_db
CHROMA_TENANT=default_tenant
CHROMA_DATABASE=default_database
CHROMA_HTTP_HOST=
CHROMA_HTTP_PORT=8000
CHROMA_HTTP_HEADERS=
CHROMA_HTTP_SSL=false
CHROMA_CLIENT_AUTH_PROVIDER=
CHROMA_CLIENT_AUTH_CREDENTIALS=

# Milvus
MILVUS_URI=/path/to/data/vector_db/milvus.db
MILVUS_DB=default
MILVUS_TOKEN=
MILVUS_INDEX_TYPE=HNSW
MILVUS_METRIC_TYPE=COSINE
MILVUS_HNSW_M=16
MILVUS_HNSW_EFCONSTRUCTION=100
MILVUS_IVF_FLAT_NLIST=128
MILVUS_DISKANN_MAX_DEGREE=56
MILVUS_DISKANN_SEARCH_LIST_SIZE=100
ENABLE_MILVUS_MULTITENANCY_MODE=false
MILVUS_COLLECTION_PREFIX=open_webui

# Qdrant
QDRANT_URI=
QDRANT_API_KEY=
QDRANT_ON_DISK=false
QDRANT_PREFER_GRPC=false
QDRANT_GRPC_PORT=6334
QDRANT_TIMEOUT=5
QDRANT_HNSW_M=16
ENABLE_QDRANT_MULTITENANCY_MODE=true
QDRANT_COLLECTION_PREFIX=open-webui

# Weaviate
WEAVIATE_HTTP_HOST=
WEAVIATE_HTTP_PORT=8080
WEAVIATE_GRPC_PORT=50051
WEAVIATE_API_KEY=

# OpenSearch
OPENSEARCH_URI=https://localhost:9200
OPENSEARCH_SSL=true
OPENSEARCH_CERT_VERIFY=false
OPENSEARCH_USERNAME=
OPENSEARCH_PASSWORD=

# ElasticSearch
ELASTICSEARCH_URL=https://localhost:9200
ELASTICSEARCH_CA_CERTS=
ELASTICSEARCH_API_KEY=
ELASTICSEARCH_USERNAME=
ELASTICSEARCH_PASSWORD=
ELASTICSEARCH_CLOUD_ID=
SSL_ASSERT_FINGERPRINT=
ELASTICSEARCH_INDEX_PREFIX=open_webui_collections

# Pgvector
PGVECTOR_DB_URL=
PGVECTOR_INITIALIZE_MAX_VECTOR_LENGTH=1536
PGVECTOR_USE_HALFVEC=false
PGVECTOR_CREATE_EXTENSION=true
PGVECTOR_PGCRYPTO=false
PGVECTOR_PGCRYPTO_KEY=
PGVECTOR_POOL_SIZE=
PGVECTOR_POOL_MAX_OVERFLOW=0
PGVECTOR_POOL_TIMEOUT=30
PGVECTOR_POOL_RECYCLE=3600
PGVECTOR_INDEX_METHOD=
PGVECTOR_HNSW_M=16
PGVECTOR_HNSW_EF_CONSTRUCTION=64
PGVECTOR_IVFFLAT_LISTS=100

# OpenGauss
OPENGAUSS_DB_URL=
OPENGAUSS_INITIALIZE_MAX_VECTOR_LENGTH=1536
OPENGAUSS_POOL_SIZE=
OPENGAUSS_POOL_MAX_OVERFLOW=0
OPENGAUSS_POOL_TIMEOUT=30
OPENGAUSS_POOL_RECYCLE=3600

# Oracle 23ai
ORACLE_DB_USER=
ORACLE_DB_PASSWORD=
ORACLE_DB_DSN=
ORACLE_DB_POOL_MIN=1
ORACLE_DB_POOL_MAX=5
ORACLE_DB_POOL_INCREMENT=1
ORACLE_DB_USE_WALLET=false
ORACLE_WALLET_DIR=
ORACLE_WALLET_PASSWORD=
ORACLE_VECTOR_LENGTH=1536

# Pinecone
PINECONE_API_KEY=
PINECONE_ENVIRONMENT=
PINECONE_CLOUD=
PINECONE_INDEX_NAME=
PINECONE_METRIC=cosine
PINECONE_DIMENSION=1536

# S3 Vector
S3_VECTOR_BUCKET_NAME=
S3_VECTOR_REGION=

# Web loaders / search
WEB_LOADER_ENGINE=
WEB_LOADER_TIMEOUT=60
WEB_FETCH_FILTER_LIST=
ENABLE_RAG_LOCAL_WEB_FETCH=false
EXTERNAL_WEB_LOADER_URL=
EXTERNAL_WEB_LOADER_API_KEY=
PLAYWRIGHT_WS_URL=
PLAYWRIGHT_TIMEOUT=60
FIRECRAWL_API_BASE_URL=
FIRECRAWL_API_KEY=
FIRECRAWL_TIMEOUT=60
TAVILY_API_KEY=
TAVILY_EXTRACT_DEPTH=3

# RAG runtime / HTTP behavior
GLOBAL_LOG_LEVEL=INFO
DOCKER=false
USE_CUDA_DOCKER=false
REQUESTS_VERIFY=true
ENABLE_FORWARD_USER_INFO_HEADERS=false
OFFLINE_MODE=false
AIOHTTP_CLIENT_TIMEOUT=
AIOHTTP_CLIENT_SESSION_SSL=

# Sentence Transformers backends
SENTENCE_TRANSFORMERS_BACKEND=
SENTENCE_TRANSFORMERS_MODEL_KWARGS={}
SENTENCE_TRANSFORMERS_CROSS_ENCODER_BACKEND=
SENTENCE_TRANSFORMERS_CROSS_ENCODER_MODEL_KWARGS={}
SENTENCE_TRANSFORMERS_CROSS_ENCODER_SIGMOID_ACTIVATION_FUNCTION=
